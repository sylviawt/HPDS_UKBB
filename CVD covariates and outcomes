####Cholesterol/HDL ratio
data$tchdl<-data$Cholesterol/data$HDL

####26002	Energy
data$Energy<-ifelse(!is.na(data$X26002.0.0),data$X26002.0.0,
                    ifelse(!is.na(data$X26002.1.0),data$X26002.1.0,
