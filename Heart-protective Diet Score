table(data$X100950.0.0, useNA = "always")


data<-mutate(data,nonwhslice = ifelse(X20091.0.0 %in% c(2,3,4,5),X100950.0.0,NA))
data<-mutate(data,nonwhslice = ifelse(X20091.0.0 %in% c(1),0,nonwhslice))
data<-mutate(data,nonwhslice = ifelse(X100950.0.0==0 & !is.na(X100950.0.0),0,nonwhslice))
table(data$nonwhslice, useNA = "always")

