data<-mutate(data,nonwhbaguette = ifelse(X20092.0.0 %in% c(1),0,nonwhbaguette))
data<-mutate(data,nonwhbaguette = ifelse(X101020.0.0==0 & !is.na(X101020.0.0),0,nonwhbaguette))
