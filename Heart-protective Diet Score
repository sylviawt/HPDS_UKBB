               X104450.0.0+
               X104460.0.0+
               X104470.0.0+
               X104490.0.0+
               X104500.0.0+
               X104510.0.0+
               X104520.0.0+
               X104530.0.0+
               X104540.0.0+
               X104550.0.0+
               X104560.0.0+
               X104570.0.0+
               X104580.0.0+
               X104590.0.0+
               X104410.0.0+
               X104420.0.0+
               X104430.0.0)
summary(data$Fruits_0)

#####103990	Vegetable consumers
table(data$X103990.1.0, useNA = "always")
#####104100	Avocado intake
table(data$X104100.1.0, useNA = "always")
data<-mutate(data,X104100.1.0 = ifelse(X103990.1.0==0,0,
                                       ifelse(X103990.1.0==1 & is.na(X104100.1.0),0,
                                              ifelse(X104100.1.0 %in% c(555),0.5,
                                                     ifelse(X104100.1.0 %in% c(444),0.25,
                                                            ifelse(X104100.1.0==300,3,X104100.1.0))))))                           
table(data$X104100.1.0, useNA = "always")

#####104440	Mixed fruit intake
table(data$X104440.1.0, useNA = "always")
data<-mutate(data,X104440.1.0 = ifelse(X103990.1.0==0,0,
                                       ifelse(X103990.1.0==1 & is.na(X104440.1.0),0,
                                              ifelse(X104440.1.0 %in% c(555),0.5,
                                                     ifelse(X104440.1.0 %in% c(400),4,X104440.1.0)))))                          
table(data$X104440.1.0, useNA = "always")

