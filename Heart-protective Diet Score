data<-mutate(data,X101470.0.0 = ifelse(X100940.0.0==0 ,0,
                                       ifelse((X100940.0.0==1 | X101300.0.0==1) & is.na(X101470.0.0),0,
                                              ifelse(X101470.0.0 %in% c(555),0.5,
                                                     ifelse(X101470.0.0 %in% c(600),6,
                                                            ifelse(X101470.0.0==200,2,X101470.0.0))))))                          
table(data$X101470.0.0, useNA = "always")

