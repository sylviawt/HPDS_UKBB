                                       ifelse(X100240.1.0==1 & is.na(X100370.1.0),0,
                                              ifelse(X100370.1.0 %in% c(300),3,
                                                     ifelse(X100370.1.0 %in% c(111),1.5,
                                                            ifelse(X100370.1.0 %in% c(555),0.5,X100370.1.0))))))                          
