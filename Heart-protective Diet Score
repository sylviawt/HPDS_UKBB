#Heart-protective diet score calculation

library(Hmisc)

data$CoatedFish_cat<-NA
data$CoatedFish_cat[which(data$CoatedFish>0)] <- cut2(data$CoatedFish[which(data$CoatedFish>0)], g=5)
data$CoatedFish_cat[which(data$CoatedFish==0)] <- 0
#Standardization to obtain scores from 0 to 5
data$CoatedFish_cat<--data$CoatedFish_cat*2.5

data$UncoatedFish_cat<-NA
data$UncoatedFish_cat[which(data$UncoatedFish>0)] <- cut2(data$UncoatedFish[which(data$UncoatedFish>0)], g=5)
data$UncoatedFish_cat[which(data$UncoatedFish==0)] <- 0
data$UncoatedFish_cat<-2.5*data$UncoatedFish_cat

data$Fruits_cat<-NA
data$Fruits_cat[which(data$Fruits>0)] <- cut2(data$Fruits[which(data$Fruits>0)], g=5)
data$Fruits_cat[which(data$Fruits==0)] <- 0

data$HomemadeSoup_cat<-NA
data$HomemadeSoup_cat[which(data$HomemadeSoup>0)] <- cut2(data$HomemadeSoup[which(data$HomemadeSoup>0)], g=5)
data$HomemadeSoup_cat[which(data$HomemadeSoup==0)] <- 0
data$HomemadeSoup_cat<-2.5*data$HomemadeSoup_cat

data$Legumes_cat<-NA
data$Legumes_cat[which(data$Legumes>0)] <- cut2(data$Legumes[which(data$Legumes>0)], g=5)
data$Legumes_cat[which(data$Legumes==0)] <- 0
data$Legumes_cat<-data$Legumes_cat*1.666667

data$Meat_cat<-NA
data$Meat_cat[which(data$Meat>0)] <- cut2(data$Meat[which(data$Meat>0)], g=5)
data$Meat_cat[which(data$Meat==0)] <- 0
data$Meat_cat <- -data$Meat_cat*1.25 

data$MilkFat_cat<-NA
data$MilkFat_cat[which(data$MilkFat>0)] <- cut2(data$MilkFat[which(data$MilkFat>0)], g=5)
data$MilkFat_cat[which(data$MilkFat==0)] <- 0
data$MilkFat_cat <- -data$MilkFat_cat 

data$Nonstarchyvegs_cat<-NA
data$Nonstarchyvegs_cat[which(data$Nonstarchyvegs>0)] <- cut2(data$Nonstarchyvegs[which(data$Nonstarchyvegs>0)], g=5)
data$Nonstarchyvegs_cat[which(data$Nonstarchyvegs==0)] <- 0

data$Potatoes_cat<-NA
data$Potatoes_cat[which(data$Potatoes>0)] <- cut2(data$Potatoes[which(data$Potatoes>0)], g=5)
data$Potatoes_cat[which(data$Potatoes==0)] <- 0
data$Potatoes_cat<--data$Potatoes_cat*1.666667

data$ReducedFat_cat<-NA
data$ReducedFat_cat[which(data$ReducedFat>0)] <- cut2(data$ReducedFat[which(data$ReducedFat>0)], g=5)
data$ReducedFat_cat[which(data$ReducedFat==0)] <- 0
data$ReducedFat_cat<-data$ReducedFat_cat*1.666667

data$RefinedGrains_cat<-NA
data$RefinedGrains_cat[which(data$RefinedGrains>0)] <- cut2(data$RefinedGrains[which(data$RefinedGrains>0)], g=5)
data$RefinedGrains_cat[which(data$RefinedGrains==0)] <- 0
data$RefinedGrains_cat<--data$RefinedGrains_cat*1.25

data$SavourySnacks_cat<-NA
data$SavourySnacks_cat[which(data$SavourySnacks>0)] <- cut2(data$SavourySnacks[which(data$SavourySnacks>0)], g=5)
data$SavourySnacks_cat[which(data$SavourySnacks==0)] <- 0
data$SavourySnacks_cat<--data$SavourySnacks_cat*1.666667

data$StarchyVegs_cat<-NA
data$StarchyVegs_cat[which(data$StarchyVegs>0)] <- cut2(data$StarchyVegs[which(data$StarchyVegs>0)], g=5)
data$StarchyVegs_cat[which(data$StarchyVegs==0)] <- 0
data$StarchyVegs_cat<-data$StarchyVegs_cat*1.25

data$SugaryDrinks_cat<-NA
data$SugaryDrinks_cat[which(data$SugaryDrinks>0)] <- cut2(data$SugaryDrinks[which(data$SugaryDrinks>0)], g=5)
data$SugaryDrinks_cat[which(data$SugaryDrinks==0)] <- 0
data$SugaryDrinks_cat<--data$SugaryDrinks_cat*2.5

data$Sweets_cat<-NA
data$Sweets_cat[which(data$Sweets>0)] <- cut2(data$Sweets[which(data$Sweets>0)], g=5)
data$Sweets_cat[which(data$Sweets==0)] <- 0
data$Sweets_cat<--data$Sweets_cat*1.25

data$ProcessedSoup_cat<-NA
data$ProcessedSoup_cat[which(data$ProcessedSoup>0)] <- cut2(data$ProcessedSoup[which(data$ProcessedSoup>0)], g=5)
data$ProcessedSoup_cat[which(data$ProcessedSoup==0)] <- 0
data$ProcessedSoup_cat<--data$ProcessedSoup_cat*2.5
table(data$ProcessedSoup_cat)

data$UnhealthyFat_cat<-NA
data$UnhealthyFat_cat[which(data$UnhealthyFat>0)] <- cut2(data$UnhealthyFat[which(data$UnhealthyFat>0)], g=5)
data$UnhealthyFat_cat[which(data$UnhealthyFat==0)] <- 0
data$UnhealthyFat_cat<--data$UnhealthyFat_cat*1.666667

data$Nuts_cat<-NA
data$Nuts_cat[which(data$Nuts>0)] <- cut2(data$Nuts[which(data$Nuts>0)], g=5)
data$Nuts_cat[which(data$Nuts==0)] <- 0
data$Nuts_cat<-data$Nuts_cat*1.25

data$TeaCoffee_cat<-NA
data$TeaCoffee_cat[which(data$TeaCoffee>0)] <- cut2(data$TeaCoffee[which(data$TeaCoffee>0)], g=5)
data$TeaCoffee_cat[which(data$TeaCoffee==0)] <- 0

data$Wholegrains_cat<-NA
data$Wholegrains_cat[which(data$Wholegrains>0)] <- cut2(data$Wholegrains[which(data$Wholegrains>0)], g=5)
data$Wholegrains_cat[which(data$Wholegrains==0)] <- 0

data$Eggs_cat<-NA
data$Eggs_cat[which(data$Eggs>0)] <- cut2(data$Eggs[which(data$Eggs>0)], g=5)
data$Eggs_cat[which(data$Eggs==0)] <- 0
data$Eggs_cat<-data$Eggs_cat*1.666667

data$ArtificialSweet_cat<-NA
data$ArtificialSweet_cat[which(data$ArtificialSweet>0)] <- cut2(data$ArtificialSweet[which(data$ArtificialSweet>0)], g=5)
data$ArtificialSweet_cat[which(data$ArtificialSweet==0)] <- 0
data$ArtificialSweet_cat<--data$ArtificialSweet_cat*1.6666667

data$HPDS<- data$ArtificialSweet_cat+data$Eggs_cat+data$Wholegrains_cat+data$TeaCoffee_cat+data$UnhealthyFat_cat+data$Sweets_cat+data$SugaryDrinks_cat+data$StarchyVegs_cat+data$SavourySnacks_cat+data$Nuts_cat+data$RefinedGrains_cat+data$ReducedFat_cat+data$Potatoes_cat+data$Nonstarchyvegs_cat+data$MilkFat_cat+data$Meat_cat+data$Legumes_cat+data$HomemadeSoup_cat+data$Fruits_cat+data$UncoatedFish_cat+data$CoatedFish_cat+data$ProcessedSoup_cat

#Split in quartiles
data$HPDS_cat <- cut2(data$HPDS, g=4)
